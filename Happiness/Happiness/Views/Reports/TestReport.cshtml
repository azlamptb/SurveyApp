@{
    ViewBag.Title = "SummaryReport";
    Layout = "~/Views/Shared/_SurveyLayout.cshtml";
}
<div class="padding">
    <div class="row">
        <div class="col-md-6">
            <div class="box">
                <div class="box-header">
                    <h2>Summary Report</h2>
                    <small></small>
                </div>
                <div class="box-divider m-a-0"></div>
                <div class="box-body">
                    <div class="form-group">
                         <label>From Date</label>
                        <div class='input-group date' id='frmdate'>
                            <input type='text' class="form-control" />
                            <span class="input-group-addon"><span class="glyphicon glyphicon-calendar"></span>
                            </span>
                        </div>
                    </div>
                    <div class="form-group">
                        <label>To Date</label>
                        <div class='input-group date' id='todate'>
                            <input type='text' class="form-control" />
                            <span class="input-group-addon"><span class="glyphicon glyphicon-calendar"></span>
                            </span>
                        </div>
                    </div>
                    <button type="submit" class="md-btn md-raised m-b-sm w-xs orange">Submit</button>
                </div>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-md-12">
            <div class="box">
                @* <div class="box-header">
                    <h2>Summary Report</h2>
                    <small></small>
                </div>*@
                <div class="box-divider m-a-0"></div>
                <div class="box-body">
                    <div class="form-group">
                        <div style="margin-top:20px">
                            <img src='@Url.Action("CreateLine","Reports")' />
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
@section Scripts {

    @Scripts.Render("~/bundles/jqueryval")
}
